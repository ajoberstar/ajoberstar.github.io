---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Andrew Oberstar's Blog</title>
	<link href="http://ajoberstar.github.io/blog/atom.xml" rel="self"/>
	<link href="http://ajoberstar.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://ajoberstar.github.io/blog</id>
	<author>
		<name>Andrew Oberstar</name>
		<email>ajoberstar@gmail.com</email>
	</author>
	{% for post in site.posts %}
		<entry>
			<title>{{ post.title | xml_escape }}</title>
			<link href="http://ajoberstar.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://ajoberstar.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
</feed>
